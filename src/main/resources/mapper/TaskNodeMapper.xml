        <result property="content" column="nodeContent" jdbcType="VARCHAR"/>
        <result property="userId" column="nodeUserId" jdbcType="INTEGER"/>
        <result property="stateId" column="nodeStateId" jdbcType="INTEGER"/>
        <result property="creatorId" column="nodeCreatorId" jdbcType="INTEGER"/>
        <result property="nodeIndex" column="nodeIndex" jdbcType="INTEGER"/>
        <result property="deadLine" column="deadLine"/>
    </resultMap>


</mapper>
